    "User Interface",
    "UI",
    "UX",
    "User Experience"
  ],
  "author": "WebDollar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebDollar/User-Interface-WebDollar/issues"
  },
  "homepage": "https://github.com/WebDollar/User-Interface-WebDollar#readme",
  "dependencies": {
    "opn": "^5.1.0",
    "stringify": "^5.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2017": "^6.24.1",
    "browserify": "^14.5.0",
    "babelify": "^7.3.0",
    "babel-plugin-module-alias": "^1.6.0"
  }
