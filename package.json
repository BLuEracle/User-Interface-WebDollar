{
  "name": "user-interface-webdollar",
  "version": "0.0.1",
  "description": "User Interface for WebDollar",
  "main": "index.js",
  "scripts": {
    "open_browser": "node src/open-browser",
    "build": "browserify src/main.js -o dist_bundle/WebDollar-User-Interface-bundle.js -t [ babelify --presets [ es2017 ] ] -t [ stringify --extensions [.html .css]  && npm run open_browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDollar/User-Interface-WebDollar.git"
  },
  "keywords": [
